<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="object">
  <xacro:include filename="$(find aist_object_database
			   )/urdf/object.urdf.xacro"/>
  <xacro:arg name="name"	  default="panel_bearing"/>
  <xacro:arg name="property_file" default="$(find aist_object_database
					   )/config/parts_properties.yaml"/>
  <xacro:arg name="collision"	  default="True"/>

  <link name="base_link"/>

  <xacro:object name="$(arg name)" prefix="$(arg name)/"
		parent="base_link" property_file="$(arg property_file)"
		collision="$(arg collision)">
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </xacro:object>
</robot>
