<?xml version="1.0" ?>
<launch>

  <arg name="config"		default="aist"/>
  <arg name="ns"		default="/"/>

  <node ns="$(arg ns)" name="run_calibration"
	pkg="aist_camera_calibration" type="run_calibration.py"
	output="screen">
    <rosparam file="$(dirname)/../config/$(arg config).yaml"/>
    <rosparam file="$(find aist_routines)/config/$(arg config)_config.yaml"/>
  </node>

</launch>
